  = link_to @park_spacerental.park_space.parknum, @park_spacerental.park_space
/p
  strong Car:
  = link_to @park_spacerental.car.platnum, @park_spacerental.car

/p
  strong Apartment Number:
  = link_to @park_spacerental.apartment.number_apartment, @park_spacerental.apartment

p
  strong Start rent:
  = @park_spacerental.start_rent
p
  strong End rent:
  = @park_spacerental.end_rent
p
  strong Price:
  = @park_spacerental.price

