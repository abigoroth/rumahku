<p id="notice"><%= notice %></p>

<h1>Infos</h1>

<table>
  <thead>
    <tr>
      <th>Email</th>
      <th>Name</th>
      <th>Apartmentnum</th>
      <th>Phonenumber</th>
      <th>Owner</th>
      <th>Password</th>
      <th>Picture</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @infos.each do |info| %>
      <tr>
        <td><%= info.email %></td>
        <td><%= info.name %></td>
        <td><%= info.apartmentnum %></td>
        <td><%= info.phonenumber %></td>
        <td><%= info.owner %></td>
        <td><%= info.password %></td>
        <td><%= info.picture %></td>
        <td><%= link_to 'Show', info %></td>
        <td><%= link_to 'Edit', edit_info_path(info) %></td>
        <td><%= link_to 'Destroy', info, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Info', new_info_path %>
